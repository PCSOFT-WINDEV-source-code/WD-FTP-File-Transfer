     code : |1-
      GLOBAL
      	// Declare a structure
      	ElementToTransfer is Structure
      		cElementType is character // Type of element to transfer (directory, file,...)
      		sElementName is string // Name of folder or file
      		sPathLocalElement is string // Path of local directory/file for the transfer
      		sPathRemoteElement is string // Path of remote directory/file for the transfer
      	END
      	
      	STgvcFTPConfiguration is Structure
      		gsFTP_Address is string				// address of FTP site
      		gsUser_Name is string			// name of the user
      		gsPWD_user is string			// password of the user
      		gbAnonymous is boolean					// anonymous connection 
      		gbAdvanced is boolean					// connection in advanced mode
      		gnPort_number is int 				// number of the FTP port of the server
      		gntimeout is int 						// time-out before abandon
      		gnConnectionType is boolean				// type of server connection (active or passive)
      		gsProfile is string					// current profile
      		gnTransferType is int				// ASCII or Binary transfer
      		gnConnectionSecurity is EConnectionSecurity		// security of the connection
      		gbIgnoreError is boolean				// do we have to ignore the certificate errors
      	END
      	
      	// Configuration of FTP Connection
      	gstgvcFTPConfiguration is STgvcFTPConfiguration
      	
      	// Configure the application
      	
      	STgvcFTPConnection is Structure
      		nConnectionID 		is int
      		sStartTime		is string
      		nTimerID			is int
      	END
      	gstgvcFTPConnection is STgvcFTPConnection
      	eTransferDirection is Enumération
      		ToServer
      		ToClient
      	END
      	CONSTANT 
      		ReturnTasksOK 	= "OK"			// Return for the cancelations of parallel tasks
      		ReturnTasksNO  = "NO" 		// Return for the cancelations of parallel tasks
      		TIMERLOCALTREEVIEW  = 42
      		TIMERREMOTETREEVIEW  = 43
      	END
      
      // Semaphore to manage the spool of transfer
      SemaphoreCreate("PoolThread",10,0,shareNone)
      
      //boolean to find out whether the parallel tasks must stop	
      bEndTask is boolean
      
      // There is no connection initially
      gstgvcFTPConnection.nConnectionID = -1
      
      // Number of pending files
      gnNbPendingFiles_cap is int
      gnNBFailedFiles_cap is int
      gnNBFilesOK_cap is int
      
      
      // Array of transfers
      garrTransferSize is array of int
      
      // Contains the IDs of current transfers
      garrCurrentTransfer is array of int 
      
      // Configure the maximum number of parallel tasks
      ParallelTaskConfigure(ptpMaximumNumberThread,6)
