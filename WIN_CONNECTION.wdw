        code : |1+
         IF COMBO_PROFILES="" RETURN
         
         IF YesNo(No, StringBuild(<§$0004§>, COMBO_PROFILES)) = Yes THEN
         	HReadSeekFirst(CONNECT,Text,COMBO_PROFILES)
         	IF HFound(CONNECT) THEN
         		HDelete(CONNECT)
         	END
         	// Update the combo box
         	ListDeleteAll(COMBO_PROFILES)
         	InitComboProfiles()
         	
         	// And the controls
         	HReadSeekFirst(CONNECT,Text,COMBO_PROFILES)
         	IF HFound() THEN
         		// Use the existing parameters
         		FillControls()
         	ELSE
         		// New profile, clear the controls
         		EmptyControls()
         	END
         END
         
