     name : StoreConfiguration
     procedure_id : 1676878240305621468
     type_code : 14
     code : |1-
      // Syntax:
      //StoreConfiguration ()
      // Store the requested configuration
      // Parameters:
      //	None
      // Return value:
      // 	None
      PROCEDURE StoreConfiguration()
               
      // Read the controls
      gstgvcFTPConfiguration.gsFTP_Address 		= EDT_SERVER_ADDRESS
      gstgvcFTPConfiguration.gsUser_Name 	= EDT_USER_NAME
      gstgvcFTPConfiguration.gsPWD_user 	= EDT_PWD
      gstgvcFTPConfiguration.gbAnonymous 			= CBOX_ANONYMOUS
      gstgvcFTPConfiguration.gbAdvanced 			= ADVANCED_INTER
      gstgvcFTPConfiguration.gnPort_number 		= EDT_PORT_NUM
      gstgvcFTPConfiguration.gnConnectionType 		= CBOX_ENABLED
      gstgvcFTPConfiguration.gsProfile 			= COMBO_PROFILES
      gstgvcFTPConfiguration.gntimeout 			= EDT_TIMEOUT
      gstgvcFTPConfiguration.gnTransferType 		= RADIO_TRANSFER
      SWITCH RADIO_SECURITY
      	// not secure
      	CASE 1
      		gstgvcFTPConfiguration.gnConnectionSecurity	= NotSecured
      	// secure: SFTP protocol
      	CASE 2
      		gstgvcFTPConfiguration.gnConnectionSecurity	=SFTP
      	// secure: FTPS protocol
      	CASE 3
      		gstgvcFTPConfiguration.gnConnectionSecurity	=FTPS
      	// secure: FTPES protocol
      	CASE 4
      	gstgvcFTPConfiguration.gnConnectionSecurity	=FTPES		
      END
      
      gstgvcFTPConfiguration.gbIgnoreError 		= CBOX_IGNORE_ERRORS[1]
