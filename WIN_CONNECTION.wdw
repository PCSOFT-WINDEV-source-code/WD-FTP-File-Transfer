     name : FillControls
     procedure_id : 1676878240305359324
     type_code : 14
     code : |1-
      // Syntax:
      //FillControls ()
      //Fill the controls of the configuration window with the current profile
      // Parameters:
      //	None
      // Return value:
      // 	None
      PROCEDURE FillControls()
      
      EDT_SERVER_ADDRESS = CONNECT.FTP_Addr
      EDT_USER_NAME = CONNECT.Login
      EDT_PWD = CONNECT.Password
      CBOX_ANONYMOUS = CONNECT.Anonymous
      ADVANCED_INTER = CONNECT.Advanced
      EDT_PORT_NUM = CONNECT.PortNum
      CBOX_ENABLED = gstgvcFTPConfiguration.gnConnectionType
      EDT_TIMEOUT = gstgvcFTPConfiguration.gntimeout
      CBOX_IGNORE_ERRORS[1] = CONNECT.IgnoreErrors
      
      IF CBOX_ANONYMOUS THEN
      	NAME_PWD..State = Grayed
      ELSE
      	NAME_PWD..State = Active
      END
