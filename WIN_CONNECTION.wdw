     name : LoadConfiguration
     procedure_id : 1676878240305883612
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      //LoadConfiguration ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      PROCEDURE LoadConfiguration()
      
      // Retrieve the profile stored
      gstgvcFTPConfiguration.gsProfile = LoadParameter("Profile","")
      
      // Read the record corresponding to the profile
      IF gstgvcFTPConfiguration.gsProfile<>"" THEN
      	HReadSeekFirst(CONNECT,Text,gstgvcFTPConfiguration.gsProfile)
      	IF HFound(CONNECT) THEN
      		// Store the parameters of the current profile
      		gstgvcFTPConfiguration.gsUser_Name 	= CONNECT.Login
      		gstgvcFTPConfiguration.gsPWD_user 	= CONNECT.Password
      		gstgvcFTPConfiguration.gbAnonymous 			= CONNECT.Anonymous
      		gstgvcFTPConfiguration.gbAdvanced 			= CONNECT.Advanced
      		gstgvcFTPConfiguration.gnPort_number 		= CONNECT.PortNum
      		gstgvcFTPConfiguration.gnConnectionType 		= CONNECT.ConnectionType
      		gstgvcFTPConfiguration.gsFTP_Address 		= CONNECT.FTP_Addr
      		gstgvcFTPConfiguration.gntimeout 			= CONNECT.ConnectionTimeout
      		gstgvcFTPConfiguration.gnTransferType 		= CONNECT.TransferType
      		gstgvcFTPConfiguration.gnConnectionSecurity		= CONNECT.ConnectionSecurity
      		
      		gstgvcFTPConfiguration.gbIgnoreError 		= CONNECT.IgnoreErrors
      	END
      END
