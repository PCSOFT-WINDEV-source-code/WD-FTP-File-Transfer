     type_code : 14
     code : |1-
      // Syntax:
      // FTPSiteConnection ()
      // Actually connect to the FTP site according to the requested configuration
      // Parameters:
      //	None
      // Return value:
      // 	None
      PROCEDURE FTPSiteConnection()
      
      HourGlass(True)
      
      // Local variables
      sProtocol is string
      
      // Find the protocol according to the security of the connection
      SWITCH gstgvcFTPConfiguration.gnConnectionSecurity
      	CASE EConnectionSecurity.NotSecured
      		// not secure	
      		sProtocol = ""
      		
      	CASE EConnectionSecurity.SFTP
      		// secure: SFTP protocol
      		sProtocol = "sftp://"
      	CASE EConnectionSecurity.FTPS
      		// secure: FTPS protocol
      		sProtocol = "ftps://"
      	CASE EConnectionSecurity.FTPES			
      		// secure: FTPES protocol	
      		sProtocol = "ftpes://"
      		
      END
      
      IF gstgvcFTPConfiguration.gbIgnoreError = True THEN
      	FTP.IgnoreError = ftpIgnoreInvalidCertificateName + ftpIgnoreInvalidCertificate + ftpIgnoreExpiredCertificate
      ELSE
      	FTP.IgnoreError = 0
      END
      
      IF gstgvcFTPConfiguration.gbAnonymous=False THEN
      	// Named connection
      	IF gstgvcFTPConfiguration.gbAdvanced=False THEN
      		// Standard setting
      		gstgvcFTPConnection.nConnectionID = FTPConnect(sProtocol+gstgvcFTPConfiguration.gsFTP_Address,gstgvcFTPConfiguration.gsUser_Name,gstgvcFTPConfiguration.gsPWD_user)		
      	ELSE
      		// Advanced setting
      		gstgvcFTPConnection.nConnectionID = FTPConnect(sProtocol+gstgvcFTPConfiguration.gsFTP_Address,gstgvcFTPConfiguration.gsUser_Name,...
      					gstgvcFTPConfiguration.gsPWD_user,gstgvcFTPConfiguration.gnPort_number,gstgvcFTPConfiguration.gnConnectionType,gstgvcFTPConfiguration.gntimeout)
      	END  	
      ELSE
      	// Anonymous connection
      	IF gstgvcFTPConfiguration.gbAdvanced=False THEN
      		// Standard setting
      		gstgvcFTPConnection.nConnectionID = FTPConnect(sProtocol+gstgvcFTPConfiguration.gsFTP_Address)
      	ELSE
      		// Advanced setting
      		gstgvcFTPConnection.nConnectionID = FTPConnect(sProtocol+gstgvcFTPConfiguration.gsFTP_Address,"","",gstgvcFTPConfiguration.gnPort_number,gstgvcFTPConfiguration.gnConnectionType,gstgvcFTPConfiguration.gntimeout)
      	END  	
      END                    
      
      HourGlass(False)
     type : 458752
   -
     name : bCompareRecord
     procedure_id : 1676878240305818076
     type_code : 14
     code : |1+
      // Syntax:
      //[ <Result> = ] cCompareRecord ()
      // Compare the current record to the requested configuration
      // Parameters:
      //	None
      // Return value:
      // 	boolean: Returns True if the information is identical
      PROCEDURE bCompareRecord()
      
      IF CONNECT.Login ~= gstgvcFTPConfiguration.gsUser_Name THEN RESULT False
      IF CONNECT.FTP_Addr ~= gstgvcFTPConfiguration.gsFTP_Address THEN RESULT False  
      IF CONNECT.Password ~= gstgvcFTPConfiguration.gsPWD_user THEN RESULT False
      IF gstgvcFTPConfiguration.gbAdvanced <> CONNECT.Advanced THEN	RESULT False	
      IF gstgvcFTPConfiguration.gbAnonymous <> CONNECT.Anonymous THEN RESULT False	
      IF gstgvcFTPConfiguration.gnPort_number <> CONNECT.PortNum THEN RESULT False	
      IF gstgvcFTPConfiguration.gnConnectionType <> CONNECT.ConnectionType THEN RESULT False	
      IF gstgvcFTPConfiguration.gntimeout <> CONNECT.ConnectionTimeout THEN RESULT False
      IF gstgvcFTPConfiguration.gnTransferType <> CONNECT.TransferType THEN RESULT False
      IF gstgvcFTPConfiguration.gnConnectionSecurity <> CONNECT.ConnectionSecurity THEN RESULT False
      IF gstgvcFTPConfiguration.gbIgnoreError <> CONNECT.IgnoreErrors THEN RESULT False
      
      // The information is identical by default
      RESULT True
      
     type : 458752
