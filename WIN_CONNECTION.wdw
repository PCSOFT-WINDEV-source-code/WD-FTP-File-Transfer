    name : BTN_CONNECTION
    identifier : 0x274577e01614a9dc
    internal_properties : 
    properties :
     x : 246
     y : 533
     width : 100
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
        type : 14
      -
        code : |1+
         // Check the input
         IF COMBO_PROFILES~="" THEN 
         	ToastDisplay(<§$0007§>,toastShort,vaMiddle)
         	RETURN
         END
         IF EDT_SERVER_ADDRESS~="" THEN 
         	ToastDisplay(<§$0006§>,toastShort,vaMiddle)
         	RETURN
         END
         
         // It is an IP address so
         IF Val(Replace(EDT_SERVER_ADDRESS,".","0"))<>0 THEN
         	// Format the IP address of the FTP site
         	IPAddrSetup()
         END
         
         // Store the configuration
         StoreConfiguration()
         
         // Compare the configuration to the previous configuration
         SWITCH nConfigComparison()
         	CASE 0	// No modification
         			// => no action
         	CASE 1   // Add
         		HAdd(CONNECT)
         	CASE 2	// Modification
         		ScreenToFile()
         		CONNECT.ConnectionSecurity= EnumerationFromValue(EConnectionSecurity,RADIO_SECURITY,EConnectionSecurity.NotSecured)
         		HModify(CONNECT)
         	
         END // End switch
         
         // Save the current profile
         SaveParameter("Profile", gstgvcFTPConfiguration.gsProfile)
         
         // if a FTP site is specified
         IF gstgvcFTPConfiguration.gsFTP_Address <> "" THEN 
         	// Open the connection
         	FTPSiteConnection()
         	// Close the window when the connection is established
         	IF gstgvcFTPConnection.nConnectionID<>-1 THEN 
         		// Move to transfer window
         		WIN_MAIN.CheckConnectionEnded()
         		WIN_MAIN.STC_Current_profile..Visible = True
         		WIN_MAIN.STC_Current_profile = "Current profile: " + gFontBold(True) +COMBO_PROFILES..DisplayedValue + gFontBold(False)
         		Close(WIN_CONNECTION)
         	ELSE
         		// Specify the reasons for non-connection
         		Error(<§$0005§>, ErrorInfo(errSummary))
         	END
         ELSE
         	// Inform the user
         	ToastDisplay(<§$0006§>,toastShort,vaMiddle)
         	RETURN
         END
         
