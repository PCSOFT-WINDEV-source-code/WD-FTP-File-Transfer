      // Syntax:
      // SetUpRecord ()
      //Store the record in memory
      // Parameters:
      //	None
      // Return value:
      // 	None
      PROCEDURE SetUpRecord()
      
      // Implement the current profile
      CONNECT.Text = gstgvcFTPConfiguration.gsProfile
      CONNECT.FTP_Addr = gstgvcFTPConfiguration.gsFTP_Address 
      CONNECT.Login = gstgvcFTPConfiguration.gsUser_Name 
      CONNECT.Password = gstgvcFTPConfiguration.gsPWD_user 
      CONNECT.Anonymous = gstgvcFTPConfiguration.gbAnonymous 
      CONNECT.Advanced = gstgvcFTPConfiguration.gbAdvanced 
      CONNECT.PortNum= gstgvcFTPConfiguration.gnPort_number 
      CONNECT.ConnectionType = gstgvcFTPConfiguration.gnConnectionType 
      CONNECT.ConnectionTimeout = gstgvcFTPConfiguration.gntimeout
      CONNECT.TransferType = gstgvcFTPConfiguration.gnTransferType
      CONNECT.ConnectionSecurity = gstgvcFTPConfiguration.gnConnectionSecurity
      CONNECT.IgnoreErrors = gstgvcFTPConfiguration.gbIgnoreError
