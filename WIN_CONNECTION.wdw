      // Syntax:
      //[ <Result> = ] nConfigComparison ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	integer : 0 if no addition or modification is required
      //           1 if adding a profile
      //		     2 if it's a modfication
      
      PROCEDURE nConfigComparison()
      
      // Find the profile
      HReadSeekFirst(CONNECT,Text,gstgvcFTPConfiguration.gsProfile)
      IF HFound(CONNECT)=False THEN
      	// Add a new profile
      	SetUpRecord()
      	RESULT 1
      ELSE
      	// Compare to the previous record
      	IF bCompareRecord() = False THEN
      		// The profile was modified
      		SetUpRecord()	
      		RESULT 2
      	ELSE
      		// No modification
      		RESULT 0
      	END                                    
      END
      
     type : 458752
