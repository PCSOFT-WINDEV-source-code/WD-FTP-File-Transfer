     type_code : 14
     code : |1-
      // Syntax:
      //EmptyControls ()
      // Clear the controls concerning the configuration
      // Parameters:
      //	None
      // Return value:
      // 	None
      PROCEDURE EmptyControls()
      
      EDT_SERVER_ADDRESS = ""
      EDT_USER_NAME = ""
      EDT_PWD = ""
      CBOX_ANONYMOUS = 0
      ADVANCED_INTER = 0
      EDT_PORT_NUM = ""
      CBOX_ENABLED = True
      EDT_TIMEOUT = 20
      CBOX_IGNORE_ERRORS[1]=False
      
      IF CBOX_ANONYMOUS THEN
      	NAME_PWD..State = Grayed
      ELSE
      	NAME_PWD..State = Active
      END
