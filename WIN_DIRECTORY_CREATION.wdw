        code : |1-
         // Local variables
         cSeparator is string
         
         // Manage the separator ("fSep" in Windows, "/" in FTP)
         IF bLocal = True THEN
         	cSeparator = fSep
         ELSE
         	cSeparator = "/"
         	IF sCurrentDir = "/" THEN cSeparator = ""
         END
         
         // Create the directory
            // Check whether it is a local directory or a remote directory
         IF bLocal = True THEN
         	// Local directory	
         	fMakeDir(sCurrentDir+cSeparator+EDT_DIRECTORY)
         	sCurrentDir = NoSpace(sCurrentDir)+NoSpace(cSeparator)+NoSpace(EDT_DIRECTORY)						
         
         ELSE
         	// Remote Directory (FTP)
         	FTPMakeDir(gstgvcFTPConnection.nConnectionID,sCurrentDir+cSeparator+EDT_DIRECTORY)
         	sCurrentDir = NoSpace(sCurrentDir)+NoSpace(cSeparator)+NoSpace(EDT_DIRECTORY)
         END
         
         
         // Close the window
         Close
         
         CASE Error:
         		Error(<ยง$0000ยง>,ErrorInfo(errSummary))
